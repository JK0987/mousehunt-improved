{
  "name": "mh-ui",
  "version": "0.11.0",
  "description": "Improve your MouseHunt experience.",
  "author": "Brad Parbs <brad@bradparbs.com> (https://bradparbs.com/)",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "run-s build:userscript build:extension",
    "build:extension": "node scripts/build-extension.mjs",
    "build:userscript": "node scripts/build-userscript.mjs",
    "ci": "run-s lint",
    "clean": "del-cli dist",
    "dev": "rollup -wc rollup.conf.js",
    "lint:css:fix": "stylelint src/**/*.css --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:fix": "run-s lint:js:fix lint:css:fix lint:md:fix",
    "lint:js:fix": "eslint --ext .js . --fix",
    "lint:js": "eslint --ext .js .",
    "lint:md:fix": "eslint --ext .md . --fix",
    "lint:md": "eslint --ext .md .",
    "lint": "run-s lint:js lint:css lint:md",
    "prebuild": "run-s ci clean",
    "sync-scripts": "node scripts/sync-scripts.mjs"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "mousehunt-utils": "^1.7.5"
  },
  "devDependencies": {
    "@wordpress/eslint-plugin": "^14.3.0",
    "del-cli": "^5.0.0",
    "esbuild": "^0.19.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-markdownlint": "^0.4.1",
    "eslint-plugin-prettier": "^5.0.0",
    "log-symbols": "^5.1.0",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "run-s": "^0.0.0",
    "stylelint": "^15.4.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3"
  }
}
