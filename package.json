{
  "name": "mh-ui",
  "version": "0.9.9",
  "description": "Improve your MouseHunt experience.",
  "author": "Brad Parbs <brad@bradparbs.com> (https://bradparbs.com/)",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build:external": "rollup -c rollup.external.conf.js",
    "build:js": "rollup -c rollup.conf.js",
    "build": "cross-env NODE_ENV=production run-s build:js",
    "ci": "run-s lint",
    "clean": "del-cli dist",
    "dev": "rollup -wc rollup.conf.js",
    "lint:css:fix": "npx stylelint src/**/*.css --fix",
    "lint:css": "npx stylelint src/**/*.css",
    "lint:fix": "run-s lint:js:fix lint:css:fix lint:md:fix",
    "lint:js:fix": "eslint --ext .js . --fix",
    "lint:js": "eslint --ext .js .",
    "lint:md:fix": "eslint --ext .md . --fix",
    "lint:md": "eslint --ext .md .",
    "lint": "run-s lint:js lint:css lint:md",
    "prebuild": "run-s ci clean",
    "sync-scripts": "node scripts/sync-external-scripts.js",
    "build:test": "node scripts/build.mjs"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@violentmonkey/dom": "^2.1.3",
    "@violentmonkey/ui": "^0.7.6"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.18.10",
    "@gera2ld/plaid-rollup": "~2.5.6",
    "@gera2ld/plaid": "~2.5.6",
    "@violentmonkey/types": "^0.1.4",
    "@wordpress/eslint-plugin": "^14.3.0",
    "del-cli": "^5.0.0",
    "esbuild": "^0.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-markdownlint": "^0.4.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "rollup-plugin-userscript": "^0.1.1",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint": "^15.4.0"
  }
}
